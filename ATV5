#include <stdio.h>
#include <math.h>

int main() {
    double x1, y1, x2, y2;
    double distancia;

    // Solicitar ao usuário para inserir as coordenadas dos pontos
    printf("Digite as coordenadas do primeiro ponto (x1 y1): ");
    scanf("%lf %lf", &x1, &y1);

    printf("Digite as coordenadas do segundo ponto (x2 y2): ");
    scanf("%lf %lf", &x2, &y2);

    // Calcular a distância entre os pontos
    distancia = sqrt(pow(x2 - x1, 2) + pow(y2 - y1, 2));

    // Imprimir a distância com 4 casas decimais após a vírgula
    printf("A distância entre os pontos (%.4lf, %.4lf) e (%.4lf, %.4lf) é %.4lf\n", x1, y1, x2, y2, distancia);

    return 0;
}
